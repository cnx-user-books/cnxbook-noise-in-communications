                      <m:mrow>
                        <m:msubsup>
                          <m:mo stretchy="false">∫</m:mo>
                          <m:mstyle fontsize="8pt">
                            <m:mrow>
                              <m:mrow>
                                <m:mstyle fontstyle="italic">
                                  <m:mrow>
                                    <m:mtext>fo</m:mtext>
                                  </m:mrow>
                                </m:mstyle>
                                <m:mo stretchy="false">−</m:mo>
                                <m:mrow>
                                  <m:mi>B</m:mi>
