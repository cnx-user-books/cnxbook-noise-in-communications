                    <m:mi/>
                    <m:mrow>
                      <m:mrow>
                        <m:mo stretchy="false">∣</m:mo>
                        <m:mi>f</m:mi>
                        <m:mo stretchy="false">∣</m:mo>
                      </m:mrow>
                      <m:mo stretchy="false">≤</m:mo>
                      <m:mfrac>
                        <m:mi>B</m:mi>
                        <m:mn>2</m:mn>
