                    <m:mrow>
                      <m:mrow>
                        <m:mrow>
                          <m:mrow>
                            <m:mi fontstyle="italic">Δf</m:mi>
                            <m:mo stretchy="false">=</m:mo>
                            <m:mover accent="true">
                              <m:msubsup>
                                <m:mi>a</m:mi>
                                <m:mstyle fontsize="8pt">
                                  <m:mrow>
                                    <m:mi>k</m:mi>
                                  </m:mrow>
                                </m:mstyle>
                                <m:mstyle fontsize="8pt">
                                  <m:mrow>
                                    <m:mn>2</m:mn>
                                  </m:mrow>
                                </m:mstyle>
                              </m:msubsup>
                              <m:mo>¯</m:mo>
                            </m:mover>
                          </m:mrow>
                          <m:mo stretchy="false">=</m:mo>
                          <m:mover accent="true">
                            <m:msubsup>
                              <m:mi>b</m:mi>
