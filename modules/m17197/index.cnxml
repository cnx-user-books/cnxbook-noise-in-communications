                        </m:mrow>
                      </m:mstyle>
                      <m:mo stretchy="false">+</m:mo>
                      <m:mover accent="true">
                        <m:mrow>
                          <m:msub>
                            <m:mn>2a</m:mn>
                            <m:mstyle fontsize="8pt">
                              <m:mrow>
