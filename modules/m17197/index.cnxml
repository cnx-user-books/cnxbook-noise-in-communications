            </m:mrow>
            <m:annotation encoding="StarMath 5.0"> size 12{P rSub { size 8{k} } = {overline  {a rSub { size 8{k} }  rSup { size 8{2} } }}  left ("cos" rSup { size 8{2} } 2πkΔ ital "ft"+"sin" rSup { size 8{2} } 2πkΔ ital "ft" right )+ {overline  {2a rSub { size 8{k} } b rSub { size 8{k} } }} "cos"2πkΔ ital "ft""sin"2πkΔ ital "ft"} {}</m:annotation>
          </m:semantics>
