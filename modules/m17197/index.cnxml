                      </m:mrow>
                      <m:mo stretchy="false">=</m:mo>
                      <m:mfrac>
                        <m:mover accent="true">
                          <m:msubsup>
                            <m:mi>c</m:mi>
                            <m:mstyle fontsize="8pt">
                              <m:mrow>
