                      </m:mfrac>
                    </m:mrow>
                  </m:mrow>
                </m:mrow>
              </m:mstyle>
              <m:mrow/>
            </m:mrow>
            <m:annotation encoding="StarMath 5.0"> size 12{P rSub { size 8{k} } =2G rSub { size 8{n} }  \( kΔf \) Δf= - 2G rSub { size 8{n} }  \( kΔf \) Δf= {overline  {a rSub { size 8{k} }  rSup { size 8{2} } }} = {overline  {b rSub { size 8{k} }  rSup { size 8{2} } }} = {  { {overline  {a rSub { size 8{k} }  rSup { size 8{2} } }} + {overline  {b rSub { size 8{k} }  rSup { size 8{2} } }} }  over  {2} } = {  { {overline  {c rSub { size 8{k} }  rSup { size 8{2} } }} }  over  {2} } } {}</m:annotation>
          </m:semantics>
