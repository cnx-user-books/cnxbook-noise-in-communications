                    <m:mstyle fontstyle="italic">
                      <m:mrow>
                        <m:mtext>ft</m:mtext>
                      </m:mrow>
                    </m:mstyle>
                    <m:mtext>sin</m:mtext>
                    <m:mn>2πkΔ</m:mn>
                    <m:mstyle fontstyle="italic">
                      <m:mrow>
