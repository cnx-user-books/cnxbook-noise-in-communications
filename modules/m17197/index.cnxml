                        </m:msubsup>
                        <m:mo>¯</m:mo>
                      </m:mover>
                    </m:mrow>
                    <m:msup>
                      <m:mtext>sin</m:mtext>
                      <m:mstyle fontsize="8pt">
                        <m:mrow>
