                    <m:mrow>
                      <m:mfenced open="(" close=")">
                        <m:mrow>
                          <m:msup>
                            <m:mtext>cos</m:mtext>
                            <m:mstyle fontsize="8pt">
                              <m:mrow>
                                <m:mn>2</m:mn>
                              </m:mrow>
                            </m:mstyle>
                          </m:msup>
                          <m:mn>2πkΔ</m:mn>
                          <m:mrow>
                            <m:mstyle fontstyle="italic">
                              <m:mrow>
                                <m:mtext>ft</m:mtext>
                              </m:mrow>
                            </m:mstyle>
                            <m:mo stretchy="false">+</m:mo>
                            <m:msup>
                              <m:mtext>sin</m:mtext>
                              <m:mstyle fontsize="8pt">
                                <m:mrow>
                                  <m:mn>2</m:mn>
                                </m:mrow>
                              </m:mstyle>
                            </m:msup>
                          </m:mrow>
                          <m:mn>2πkΔ</m:mn>
