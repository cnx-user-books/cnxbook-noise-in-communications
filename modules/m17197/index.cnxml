            <m:annotation encoding="StarMath 5.0"> size 12{ {overline  {a rSub { size 8{k} }  rSup { size 8{2} } }} = {overline  {b rSub { size 8{k} }  rSup { size 8{2} } }} } {}</m:annotation>
          </m:semantics>
        </m:math>
